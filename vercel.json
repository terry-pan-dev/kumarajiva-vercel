{
    "regions": ["syd1"],
    "buildCommand": "pnpm build && pnpm db:migrate",
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                "key": "Content-Security-Policy",
                "value": "connect-src 'self' https://*.ingest.us.sentry.io https://sentry.io;"
                }
            ]
        }
    ]
}
